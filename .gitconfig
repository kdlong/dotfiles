[core]
    editor = vim
[user]
    name = Kenneth Long
    email = kdlong@hep.wisc.edu
	github = kdlong
[alias]
    s = status
    c = commit
    b = branch --verbose
    co = checkout
    unstage = reset HEAD --
    l = log --decorate --stat
    graphviz = "!f() { echo 'digraph git {' ; git log --pretty='format:  %h -> { %p }' \"$@\" | sed 's/[0-9a-f][0-9a-f]*/\"&\"/g' ; echo '}'; }; f"
[push]
    default = simple
[diff]
	tool = vimdiff
